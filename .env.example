
# Copy this file to .env and adjust values

PROJECT_NAME="VitaAvanza Backend"
API_V1_STR="/api"

# For local dev SQLite is fine. For Render/Prod use Postgres DATABASE_URL
DATABASE_URL="sqlite:///./vitaavanza.db"

JWT_SECRET_KEY="CHANGE_ME_SUPER_SECRET"
JWT_ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# CORS – change to your frontend domains in production
BACKEND_CORS_ORIGINS='["http://localhost:5173", "https://vitaavanza.com"]'

# OpenAI – optional but recommended for Mitra
OPENAI_API_KEY=""
